{
  "name": "intervalio",
  "version": "0.0.1",
  "description": "Do things on an interval",
  "engineStrict": true,
  "main": "./build/src/main.js",
  "types": "./build/src/main.d.ts",
  "engines": {
    "node": ">= 8.9.0"
  },
  "devDependencies": {
    "@types/jest": "~22.2.0",
    "@types/node": "~8.5.8",
    "@types/nconf": "~0.0.37",
    "jest": "~22.4.1",
    "rimraf": "~2.6.2",
    "tslint": "~5.9.1",
    "tslint-microsoft-contrib": "~5.0.2",
    "tsutils": "~2.18.0",
    "typescript": "~2.6.2",
    "ts-jest": "~22.4.1",
    "webpack": "~3.10.0",
    "webpack-merge": "~4.1.1",
    "ts-loader": "~3.3.1",
    "file-loader": "~1.1.6",
    "merge-jsons-webpack-plugin": "~1.0.12"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp logs",
    "build": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "lint-autofix": "tslint -t stylish --fix --project \"tsconfig.json\"",
    "pretest": "npm run lint --force",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch",
    "test-basic": "jest",
    "start": "node ./build/main.js"
  },
  "author": "Mitch Chiles <mitchchiles@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/MitchCodes/Intervalio.git"
  },
  "bugs": {
    "url": "https://github.com/MitchCodes/Intervalio/issues"
  },
  "homepage": "https://github.com/MitchCodes/Intervalio",
  "dependencies": {
    "tslib": "~1.8.1",
    "moment": "^2.24.0",
    "winston": "3.2.1",
    "winston-daily-rotate-file": "3.8.0",
    "winston-transport": "4.3.0",
    "nconf": "~0.10.0"
  }
}
